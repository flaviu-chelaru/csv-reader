{
  "name": "cfv1000/csv-reader",
  "description": "Another PHP csv reader",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "csv",
    "custom line endings",
    "iterator csv",
    "pause or resume via tell and seek"
  ],
  "require": {
    "php": "~7.1 || ~8.0"
  },
  "autoload": {
    "psr-4": {
      "cfv1000\\CsvReader\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "cfv1000\\CsvReader\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "mikey179/vfsstream": "^1.6",
    "rector/rector": "^1.2"
  }
}
